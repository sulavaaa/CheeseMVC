@model List<CheeseMVC.ViewModels.RemoveCheeseViewModel>
<h1>Remove</h1>
<form method="post">
    @foreach (var cheese in Model) 
    {
       
        <input type="checkbox" name="cheeseIds" id ="@cheese.ID" value="@cheese.ID" />
        <label for="@cheese.ID">@cheese.Name</label>
        <br />
    }
    <input type="submit" name="submit" value="Remove Selected Cheese(s)" />
</form>